{
  "manifest_version": 3,
  "name": "Jumpmark Dock",
  "version": "1.0.1",
  "description": "ページごとに個別のリンク集（ショートカット）を管理・呼び出せるChrome拡張です。",
  "icons": {
    "16": "icons/icon_16x16.png",
    "32": "icons/icon_32x32.png",
    "48": "icons/icon_48x48.png",
    "128": "icons/icon_128x128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon_16x16.png",
      "48": "icons/icon_48x48.png",
      "128": "icons/icon_128x128.png"
    },
    "default_title": "Jumpmark Dock"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "author": "maepon",
  "homepage_url": "https://github.com/maepon/jumpmark-dock"
}